{
  "private": true,
  "name": "aws-lambda-helper-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --port 8081 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "preact-cli": "^3.0.3",
    "preact-cli-plugin-env-vars": "^1.2.1",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.0.0",
    "install": "^0.13.0",
    "npm": "^6.14.8",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
